{
  "name": "tape-pencil",
  "description": "A small helper for tape to inject dependencies into your tests",
  "author": "Andy Shea <aa.shea@gmail.com>",
  "version": "0.1.2",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/andy-shea/tape-pencil"
  },
  "homepage": "https://github.com/andy-shea/tape-pencil",
  "keywords": [
    "tap",
    "tape",
    "test",
    "provision",
    "dependency",
    "injection"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc tape ./test/**/*.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prebuild": "npm test",
    "build": "babel src -d lib",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "tape": "^4.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.1.3",
    "nyc": "^8.3.2",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6"
  }
}
